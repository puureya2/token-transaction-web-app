  "scripts": {
    "start": "npm-run-all --parallel dev client ",
    "prepare": "npm-run-all --parallel server client ",
    "server": "node server/server.js",
    "dev": "npx nodemon server/server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile": "npm-run-all compile-token compile-factory compile-bridge compile-estokk",
    "compile-test": "solcjs --bin --abi --include-path node_modules/ --base-path . resource/contracts/1_Storage.sol -o resource/build/1_Storage",
    "compile-factory": "solcjs --bin --abi --include-path node_modules/ --base-path . resource/contracts/TokenFactory.sol -o resource/build/TokenFactory",
    "compile-token": "solcjs --bin --abi --include-path node_modules/ --base-path . resource/contracts/Token.sol -o resource/build/Token",
    "compile-bridge": "solcjs --bin --abi --include-path node_modules/ --base-path . resource/contracts/BridgeToken.sol -o resource/build/BridgeToken",
    "compile-estokk": "solcjs --bin --abi --include-path node_modules/ --base-path . resource/contracts/EstokkTokenYam.sol -o resource/build/EstokkTokenYam",
    "temp": "node temp.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
